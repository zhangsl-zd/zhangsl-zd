		#gpio-cells = <2>;
		status = "disabled";
	};

	macb0: ethernet@32010000 {
		compatible = "cdns,phytium-gem";
		reg = <0x0 0x32010000 0x0 0x2000>;
		interrupts = <GIC_SPI 64 IRQ_TYPE_LEVEL_HIGH>,
